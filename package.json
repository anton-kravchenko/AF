{
  "name": "angular",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}' '__tests__/**/*.{ts,tsx}'",
    "pretest": "npm run lint",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
    "verbose": true
  },
  "devDependencies": {
    "@types/jest": "20.0.8",
    "jest": "21.0.1",
    "ts-jest": "21.0.0",
    "tslint": "5.7.0",
    "typescript": "2.5.2"
  }
}
